{
    "manifest_version": 3,
    "name": "NEPSE Extension",
    "version": "1.0.0",
    "description": "Dsplays the Live Stock Status.",
    "content_scripts": [{
        "matches":[
           "<all_urls>"
        ],
        "js": ["js/content.js"]
    }],
    "action": {
        "default_icon":{
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        },
        "default_popup": "Popup.html",
        "default_title": "NEPSE Live"
    }
}
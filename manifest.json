{
    "manifest_version": 3,
    "short_name": "NEPSE Extension",
    "name": "NEPSE Browser Extension",
    "version": "1.0.0",
    "description": "NEPSE Extension : A browser extension to display information about shares listed in NEPSE.",
    "content_scripts": [{
        "matches":[
           "<all_urls>"
        ],
        "js": ["js/content.js"]
    }],
    "action": {
        "default_icon":{
            "19": "img/icon19.png",
            "38": "img/icon38.png",
            "128": "img/icon128.png"
        },
        "default_popup": "Popup.html",
        "default_title": "NEPSE Live"
       
    },
    "icons":{
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
        
    }
   
}